' + oMin\n      break\n    case 'hh:mm:ss':\n      oTime = oTime + oHour + ':' + oMin + ':' + oSec\n      break\n  }\n  return oTime\n}","/Users/ll/demo/lx/react/react-hooks/src/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/Modal/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/AddInput/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/Modal/EditModal/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/noDataTip/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/NoDataTip/index.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":43,"column":3,"nodeType":"53","messageId":"54","endLine":53,"endColumn":4},"no-native-reassign",["55"],"no-negated-in-lhs",["56"],"default-case","Expected a default case.","SwitchStatement","missingDefaultCase","no-global-assign","no-unsafe-negation"]