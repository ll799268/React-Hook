Path":"33","messages":"34","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"35","messages":"36","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"37","messages":"38","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"39","messages":"40","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"41","messages":"42","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/ll/demo/lx/react/react-hooks/src/components/Modal/CheckModal/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/App.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/List/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/libs/utils.js",["43"],"/**\n * 时间戳转化为年月日\n * @param times 时间戳\n * @param ymd 格式类型(yyyy-mm-dd,yyyy/mm/dd)\n * @param hms 可选,格式类型(hh,hh:mm,hh:mm:ss)\n * @returns {年月日}\n */\nexport const formatTime = (times, ymd, hms) => {\n  const oDate = new Date(times),\n   oYear = oDate.getFullYear()\n\n  let oMonth = oDate.getMonth() + 1,\n   oDay = oDate.getDate(),\n   oHour = oDate.getHours(),\n   oMin = oDate.getMinutes(),\n   oSec = oDate.getSeconds(),\n   oTime = '' // 最后拼接时间\n\n  // 比较时分秒是否小于十\n  oHour = ('0' + oHour).slice(-2)\n  oMin = ('0' + oMin).slice(-2)\n  oSec = ('0' + oSec).slice(-2)\n\n  // 比较日期是否小于十\n  oMonth = ('0' + oMonth).slice(-2)\n  oDay = ('0' + oDay).slice(-2)\n\n  // 年月日格式\n  switch (ymd) {\n    case 'yyyy-mm-dd':\n      oTime = oYear + '-' + oMonth + '-' + oDay + ' '\n      break\n    case 'yyyy/mm/dd':\n      oTime = oYear + '/' + oMonth + '/' + oDay + ' '\n      break\n    case 'yyyy年mm月dd日':\n      oTime = oYear + '年' + oMonth + '月' + oDay + '日 '\n      break\n    default:\n      break\n  }\n  // 时分秒格式\n  switch (hms) {\n    case 'hh':\n      oTime = oTime + oHour\n      break\n    case 'hh:mm':\n      oTime = oTime + oHour + ':' + oMin\n      break\n    case 'hh:mm:ss':\n      oTime = oTime + oHour + ':' + oMin + ':' + oSec\n      break\n  }\n  return oTime\n}","/Users/ll/demo/lx/react/react-hooks/src/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/Modal/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/AddInput/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/Modal/EditModal/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/noDataTip/index.js",[],"/Users/ll/demo/lx/react/react-hooks/src/components/NoDataTip/index.js",[],{"ruleId":"44","severity":1,"message":"45","line":43,"column":3,"nodeType":"46","messageId":"47","endLine":53,"endColumn":4},"default-case","Expected a default case.","SwitchStatement","missingDefaultCase"]